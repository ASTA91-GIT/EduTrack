{
  "name": "edutrack",
  "version": "1.0.0",
  "description": "Automated Student Attendance Monitoring & Analytics System",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm start",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client\"",
    "server:dev": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "setup": "npm run install-all && echo 'Setup complete! Run npm start to start the application'"
  },
  "keywords": [
    "attendance",
    "monitoring",
    "education",
    "analytics",
    "face-recognition",
    "qr-code",
    "student-management"
  ],
  "author": "EduTrack Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/edutrack.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/edutrack/issues"
  },
  "homepage": "https://github.com/your-username/edutrack#readme"
}
